	partition DimFecha = calculated
		mode: import
		source =
				VAR MinFecha = CALCULATE(MIN(FactVentas[Fecha]), NOT(ISBLANK(FactVentas[Fecha])))
				VAR MaxFecha = CALCULATE(MAX(FactVentas[Fecha]), NOT(ISBLANK(FactVentas[Fecha])))
				RETURN
				ADDCOLUMNS(
				    CALENDAR(MinFecha, MaxFecha),
				    "Año", YEAR([Date]),
				    "MesNumero", MONTH([Date]),
				    "MesNombre", FORMAT([Date], "[$-C0A]MMMM"),
				    "AñoMes", FORMAT([Date], "YYYY-MM"),
				    "Trimestre", QUARTER([Date]),
				    "TrimestreNombre", "T" & QUARTER([Date]) & " " & YEAR([Date]),
				    "Dia", DAY([Date]),
				    "NombreDiaSemana", FORMAT([Date], "[$-C0A]DDDD"),
				    "NumeroDiaSemana", WEEKDAY([Date],2),
				    "EsFinDeSemana", IF(WEEKDAY([Date],2)>5, TRUE(), FALSE())
				)

